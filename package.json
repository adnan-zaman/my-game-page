{
  "name": "mygamepage",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "setupSecret": "cross-env SECRET=abcdefg",
    "setupDb": "cross-env DB=my_game_page_test DBUSER=root DBPASS=password DBPORT=3306",
    "test": "cross-env-shell npm run setupSecret npm run setupDb \"mocha --recursive ./tests/*.test.js\"",
    "indexTest": "powershell $ENV:SECRET='abcdefg'; $ENV:DB='my_game_page_test'; $ENV:DBUSER='root'; $ENV:DBPASS='password'; $ENV:DBPORT='3306'; node tests/signupPostTest.js",
    "devstart": "powershell $ENV:DEBUG='mygamepage:* index app passport'; $ENV:SECRET='abcdefg'; $ENV:DB='my_game_page_dev'; $ENV:DBUSER='root'; $ENV:DBPASS='password'; $ENV:DBPORT='3306';  nodemon --ignore sessions ./bin/www;"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-session": "^1.17.1",
    "express-validator": "^6.6.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "mysql": "^2.18.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pug": "^3.0.0",
    "session-file-store": "^1.5.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.6",
    "sinon": "^9.2.1"
  }
}
